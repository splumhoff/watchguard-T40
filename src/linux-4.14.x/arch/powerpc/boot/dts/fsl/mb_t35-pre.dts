/include/ "t102xsi-pre.dtsi"

/ {
	model = "fsl,T1024RDB";
	compatible = "fsl,T1024RDB";
	#address-cells = <2>;
	#size-cells = <2>;
	interrupt-parent = <&mpic>;

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		bman_fbpr: bman-fbpr {
			size = <0 0x1000000>;
			alignment = <0 0x1000000>;
			compatible = "fsl,bman-fbpr";
			alloc-ranges = <0 0 0x10000 0>;
		};

		qman_fqd: qman-fqd {
			size = <0 0x400000>;
			alignment = <0 0x400000>;
			compatible = "fsl,qman-fqd";
			alloc-ranges = <0 0 0x10000 0>;
		};

		qman_pfdr: qman-pfdr {
			size = <0 0x2000000>;
			alignment = <0 0x2000000>;
			compatible = "fsl,qman-pfdr";
			alloc-ranges = <0 0 0x10000 0>;
		};
	};

	ifc: localbus@ffe124000 {
		reg = <0xf 0xfe124000 0 0x2000>;
		ranges = <0 0 0xf 0xe8000000 0x08000000
			  2 0 0xf 0xff800000 0x00010000
			  3 0 0xf 0xffdf0000 0x00008000>;
	};

	memory {
		device_type = "memory";
	};

	dcsr: dcsr@f00000000 {
		ranges = <0x00000000 0xf 0x00000000 0x01072000>;
	};

	bportals: bman-portals@ff4000000 {
		ranges = <0x0 0xf 0xf4000000 0x2000000>;
	};

	qportals: qman-portals@ff6000000 {
		ranges = <0x0 0xf 0xf6000000 0x2000000>;
	};

	soc: soc@ffe000000 {
		ranges = <0x00000000 0xf 0xfe000000 0x1000000>;
		reg = <0xf 0xfe000000 0 0x00001000>;
		spi@110000 {
			flash@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				#compatible = "fsl,espi-flash";
				compatible = "fsl,mx25u3235f", "jedec,spi-nor";
				reg = <0>;
				linux,modalias = "fsl_m25p80";
				modal = "mx25u3235f";
				spi-max-frequency = <50000000>;
				mode = <0>;
				partition@00000000 {
					/*1MB: Map it later: u-boot */
					reg = <0x00000000 0x00100000>;
					label = "NOR (RW) WG U-Boot Image";
				};
				partition@00100000 {
					/*64KB: u-boot ENV */
					reg = <0x00100000 0x00010000>;
					label = "NOR (RW) WG U-Boot ENV";
				};
				partition@00110000 {
					/*128KB: fman ucode */
					reg = <0x00110000 0x00020000>;
					label = "NOR (RW) FMAN UCODE";
				};

				partition@00130000 {
					/*832KB: empty-hole:  */
					reg = <0x00130000 0x000D0000>;
					label = "NOR (RW) WG EMPYT-HOLE";
				};

				partition@00200000 {
					/*128KB*/
					reg = <0x00200000 0x00020000>;
					label = "NOR (RW) WG CFG0";
				};

				partition@00220000 {
					/*64KB*/
					reg = <0x00220000 0x00010000>;
					label = "NOR (RW) WG CFG1";
				};

				partition@00230000 {
					/*128KB*/
					reg = <0x00230000 0x00020000>;
					label = "NOR (RW) WG MFG";
				};

				partition@00250000 {
					/*704KB*/
					reg = <0x00250000 0x000B0000>;
					label = "NOR (RW) WG bootOpt Data & reserved";
				};
				partition@00300000 {
					/*1MB*/
					reg = <0x00300000 0x00100000>;
					label = "NOR (RW) WG extra reserved";
				};

			};

			slic@1 {
				compatible = "maxim,ds26522";
				reg = <1>;
				spi-max-frequency = <2000000>;
			};

			slic@2 {
				compatible = "maxim,ds26522";
				reg = <2>;
				spi-max-frequency = <2000000>;
			};
		};

		i2c@118000 {
			adt7461@4c {
				/* Thermal Monitor */
				compatible = "adi,adt7461";
				reg = <0x4c>;
			};

			eeprom@50 {
				compatible = "at24,24c256";
				reg = <0x50>;
			};

			rtc@68 {
				compatible = "dallas,ds1339";
				reg = <0x68>;
				interrupts = <0x1 0x1 0 0>;
			};
			s35390a: s35390a@30 {
				compatible = "s35390a";
				reg = <0x30>;
			};
		};

		i2c@118100 {
			pca9546@77 {
				compatible = "nxp,pca9546";
				reg = <0x77>;
				#address-cells = <1>;
				#size-cells = <0>;
			};
			tpm@29 {
                                compatible = "tpm,tpm_i2c_atmel";
                                reg = <0x29>;
			};
		};

		fman@400000 {
			sleep = <&rcpm 0x00000008>;

			enet0: ethernet@e0000 {
				status = "disabled";
			};

			enet1: ethernet@e2000 {
				status = "disabled";
			};

			enet2: ethernet@e4000 {
				fixed-link = <13 1 1000 0 0>;
				phy-connection-type = "rgmii";
				sleep = <&rcpm 0x20000000>;
			};

			enet3: ethernet@e6000 {
				fixed-link = <14 1 1000 0 0>;
				phy-connection-type = "rgmii";
				sleep = <&rcpm 0x10000000>;
			};

			mdio0: mdio@fc000 {
			};

			xmdio0: mdio@fd000 {
				xg_aqr105_phy3: ethernet-phy@1 {
					compatible = "ethernet-phy-ieee802.3-c45";
					reg = <0x1>;
				};
			};
		};
	};

	pci0: pcie@ffe240000 {
		reg = <0xf 0xfe240000 0 0x10000>;
		ranges = <0x02000000 0 0xe0000000 0xc 0x00000000 0 0x10000000
			  0x01000000 0 0x00000000 0xf 0xf8000000 0 0x00010000>;
		pcie@0 {
			ranges = <0x02000000 0 0xe0000000
				  0x02000000 0 0xe0000000
				  0 0x10000000

				  0x01000000 0 0x00000000
				  0x01000000 0 0x00000000
				  0 0x00010000>;
		};
	};

	pci1: pcie@ffe250000 {
		reg = <0xf 0xfe250000 0 0x10000>;
		ranges = <0x02000000 0 0xe0000000 0xc 0x10000000 0 0x10000000
			  0x01000000 0 0x00000000 0xf 0xf8010000 0 0x00010000>;
		pcie@0 {
			ranges = <0x02000000 0 0xe0000000
				  0x02000000 0 0xe0000000
				  0 0x10000000

				  0x01000000 0 0x00000000
				  0x01000000 0 0x00000000
				  0 0x00010000>;
		};
	};

	pci2: pcie@ffe260000 {
		reg = <0xf 0xfe260000 0 0x10000>;
		ranges = <0x02000000 0 0xe0000000 0xc 0x20000000 0 0x10000000
			  0x01000000 0 0x00000000 0xf 0xf8020000 0 0x00010000>;
		pcie@0 {
			ranges = <0x02000000 0 0xe0000000
				  0x02000000 0 0xe0000000
				  0 0x10000000

				  0x01000000 0 0x00000000
				  0x01000000 0 0x00000000
				  0 0x00010000>;
		};
	};
};

/include/ "t1024si-post.dtsi"
