config MT76_CORE
	tristate
	depends on m

config MT76_LEDS
	bool
	depends on MT76_CORE
	depends on LEDS_CLASS=y || MT76_CORE=LEDS_CLASS
	default y

config MT76_USB
	tristate
	depends on m
	depends on MT76_CORE

config MT76x02_LIB
	tristate
	depends on m
	select MT76_CORE

config MT76x02_USB
	tristate
	depends on m
	select MT76_USB

source "drivers/net/wireless/mediatek/mt76/mt76x0/Kconfig"
source "drivers/net/wireless/mediatek/mt76/mt76x2/Kconfig"
