+
+	/* Set output mode */
+	tmp = inl_p(CFXII_PCH_BASE+CFXII_PCH_LED_IO_SEL);
+	tmp &= ~(CFXII_ARM_MASK);
+	outl_p(tmp, CFXII_PCH_BASE+CFXII_PCH_LED_IO_SEL);
