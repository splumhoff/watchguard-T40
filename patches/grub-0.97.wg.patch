+/* lcd */
+static int
+lcd_func (char *arg, int flags)
+{
+  int line_num;
+
+  /* set the line number */
+  if (! safe_parse_maxint (&arg, &line_num))
+    return 1;
+#ifndef GRUB_UTIL
+  if (LCD_MODULE_A == lcd_module)
+  {
+    ezio_cmd (CMD_SETPOS + (line_num == 2 ? LCD_ROW2_OFFSET : 0));
+  } 
+#endif
+  arg = skip_to (0, arg);
+
+  /* display the string */
+#ifndef GRUB_UTIL
+  if (LCD_MODULE_A == lcd_module)
+  {
+    ezio_putchars(arg);
+  }
+  else if (LCD_MODULE_B == lcd_module)
+  {
+    LCM_Write(line_num, arg);
+  }
+#endif
+
+  return 0;
+}
