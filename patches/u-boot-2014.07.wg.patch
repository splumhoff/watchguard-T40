+
+void spi_cs_activate(struct spi_slave *slave)
+{
+	struct fsl_spi_driver *driver = find_fsl_spi_driver(slave->bus,
+								slave->cs);
+
+	if (driver && driver->cs_activate)
+		driver->cs_activate(slave);
