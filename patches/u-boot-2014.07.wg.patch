+
+void spi_cs_deactivate(struct spi_slave *slave)
+{
+	struct fsl_spi_driver *driver = find_fsl_spi_driver(slave->bus,
+								slave->cs);
+
+	if (driver && driver->cs_deactivate)
+		driver->cs_deactivate(slave);
+}
