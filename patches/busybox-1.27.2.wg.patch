--- busybox-1.27.2/loginutils/login.c.orig	2022-05-09 13:47:45.070507501 -0700
+++ busybox-1.27.2/loginutils/login.c	2022-05-09 13:47:46.742439627 -0700
@@ -391,7 +391,10 @@
 	/* Was breaking "login <username>" from shell command line: */
 	/*bb_setpgrp();*/
 
-	openlog(applet_name, LOG_PID | LOG_CONS, LOG_AUTH);
+ 	/* WatchGuard: port WG 1.18.2 changes --
+	 * avoid information leakage, do not log to console 
+	 */
+	openlog(applet_name, LOG_PID, LOG_AUTH);
 
 	while (1) {
 		/* flush away any type-ahead (as getty does) */
@@ -588,6 +591,23 @@
 	// bb_setpgrp();
 	// If this stuff is really needed, add it and explain why!
 
+	/* Watchguard: this stuff is really needed.  
+	 * Unless init peeks into the getty command line and
+	 * does this itself, we wind up with
+	 *
+	 * -ash: can't access tty; job control turned off
+	 *
+	 * Init was doing this with /dev/console before, not by peeking
+	 * into the command line, which caused loss of job control for
+	 * the first login process on any console or serial terminal,
+	 * when the second logged in.  That is being removed from init,
+	 * and now even the first process in has no job control.
+	 */
+
+	setsid();
+	ioctl(0, TIOCSCTTY, 1);
+	bb_setpgrp();
+
 	/* Set signals to defaults */
 	/* Non-ignored signals revert to SIG_DFL on exec anyway */
 	/*signal(SIGALRM, SIG_DFL);*/
